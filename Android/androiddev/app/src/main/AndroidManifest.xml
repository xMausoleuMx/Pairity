<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="comxmausoleumx.httpsgithub.pairity">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Splash_Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".mainpage">
        </activity>
        <activity
                android:name=".mainPageSettings"
                android:label="@string/title_activity_main_page_settings">
        </activity>
        <activity android:name=".createNewPairingPage">
        </activity>
        <activity android:name=".linkExistingPage">
        </activity>
        <activity android:name=".removeLinkPage">
        </activity>
    </application>

</manifest>